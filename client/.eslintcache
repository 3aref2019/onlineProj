gError, notFound } = useSelector(\n    (state: State) => state.sub\n  );\n  const course = useSelector((state: State) => state.course);\n  return (\n    <div className=\"container mx-auto my-12 lg:px-12 \">\n      <div className=\"mb-2 ml-2\">\n        {/* BREADCRUMB*/}\n        <Breadcrumb>\n          <Breadcrumb.Item>\n            <Link to=\"/\">\n              HOMEPAGE\n              <i className=\"ml-2 fas fa-home\"></i>\n            </Link>\n          </Breadcrumb.Item>\n          <Breadcrumb.Item>\n            {!loading && !loadingError && !notFound && (\n              <>\n                <Breadcrumb.Item>\n                  <Link to={`/c/courses/${sub.courseSlug}`}>\n                    {sub.courseName}\n                  </Link>{\" \"}\n                  /{\" \"}\n                </Breadcrumb.Item>\n                <Breadcrumb.Item>\n                  <Dropdown\n                    trigger={[\"click\"]}\n                    overlay={() => (\n                      <Menu>\n                        {!course.loading &&\n                          course.course.subs.map((sub) => {\n                            return (\n                              <Menu.Item key={sub.slug}>\n                                <Link\n                                  className=\"m-3\"\n                                  to={`/s/${course.course.slug}/${sub.slug}`}\n                                >\n                                  {sub.title}\n                                </Link>\n                              </Menu.Item>\n                            );\n                          })}\n                      </Menu>\n                    )}\n                  >\n                    <span className=\"cursor-pointer\">\n                      {!loading && sub.title} <DownOutlined />\n                    </span>\n                  </Dropdown>\n                </Breadcrumb.Item>\n              </>\n            )}\n          </Breadcrumb.Item>\n        </Breadcrumb>\n      </div>\n      <div>\n        <div className=\"flex flex-col w-full \">\n          {loading ? (\n            <div className=\"flex items-center justify-center h-32 col-span-4 max-h\">\n              <Spin size=\"large\" spinning />\n            </div>\n          ) : loadingError ? (\n            <Result\n              status=\"500\"\n              title=\"Problem loading page\"\n              extra={\n                <Button type=\"primary\" onClick={() => load()}>\n                  RELOAD\n                </Button>\n              }\n            />\n          ) : notFound ? (\n            <div className=\"col-span-4 p-1 bg-white md:col-span-3\">\n              <NotFound />\n            </div>\n          ) : sub.lessons.length === 0 || shownLessons === 0 ? (\n            <>\n              <Result\n                status=\"info\"\n                title=\"This section is empty\"\n                subTitle=\"visit it later...\"\n                extra={\n                  <Button\n                    type=\"primary\"\n                    onClick={() => history.push(`/c/courses/${sub.courseSlug}`)}\n                  >\n                    GO BACK\n                  </Button>\n                }\n              />\n            </>\n          ) : (\n            sub.lessons.map(\n              (lesson) =>\n                lesson.displayStatus && (\n                  <div\n                    key={lesson.id}\n                    className=\"flex flex-col justify-between px-1 py-2 mx-5 mb-4 bg-gray-100 border-t-2 border-b-2 border-l-2 border-r-8 border-gray-500 md:flex-row justify-items-center\"\n                  >\n                    <PostComponent\n                      identifier={lesson.identifier}\n                      slug={lesson.slug}\n                      title={lesson.title}\n                      updatedAt={lesson.updatedAt}\n                      subSlug={subSlug}\n                    />\n                  </div>\n                )\n            )\n          )}\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default Sub;\n","C:\\OnlineCourse\\V2\\client\\src\\components\\Articles\\Articles.tsx",["361"],"//React\nimport React, { useEffect } from \"react\";\nimport { Link, useHistory, useLocation } from \"react-router-dom\";\n//Redux\nimport { useDispatch, useSelector } from \"react-redux\";\n\n//Types\nimport { State } from \"../../redux/types\";\n\n//Components\n\n//Antd\nimport { Breadcrumb, Button, Result, Pagination, Spin } from \"antd\";\n\n//Actions\nimport {\n  getArticles,\n  getArticlesPaginated,\n} from \"../../redux/slices/articlesSlice\";\nfunction Article() {\n  const location = useLocation();\n  const dispatch = useDispatch();\n  const history = useHistory();\n  const { error, status, infos, listPaginated } = useSelector(\n    (state: State) => state.articles\n  );\n  const query =\n    location.search !== \"\"\n      ? JSON.parse(\n          '{\"' +\n            location.search\n              .substring(1)\n              .replace(/&/g, '\",\"')\n              .replace(/=/g, '\":\"') +\n            '\"}',\n          function (key, value) {\n            return key === \"\" ? value : decodeURIComponent(value);\n          }\n        )\n      : { current: 1 };\n  useEffect(() => {\n    dispatch(\n      getArticlesPaginated({\n        current: Number(query.current),\n      })\n    );\n  }, [dispatch, query.current]);\n  return (\n    <div className=\"container px-5 py-4 \">\n      <Breadcrumb>\n        <Breadcrumb.Item>\n          <Link to=\"/\">\n            Homepage\n            <i className=\"ml-2 fas fa-home\"></i>\n          </Link>\n        </Breadcrumb.Item>\n        <Breadcrumb.Item>Articles</Breadcrumb.Item>\n      </Breadcrumb>\n      {status ? (\n        <div className=\"flex items-center justify-center h-96\">\n          <Spin size=\"large\" spinning />\n        </div>\n      ) : error ? (\n        <Result\n          status=\"500\"\n          title=\"unexpected problem\"\n          extra={\n            <Button type=\"primary\" onClick={getArticles}>\n              reload page\n            </Button>\n          }\n        />\n      ) : listPaginated.length === 0 ? (\n        <>\n          <Result\n            status=\"info\"\n            title=\"There's no articles\"\n            subTitle=\"try again later..\"\n            extra={\n              <Button type=\"primary\">\n                <Link to=\"/\">return</Link>\n              </Button>\n            }\n          />\n        </>\n      ) : (\n        <div className=\"flex flex-col items-center \">\n          {listPaginated.map((article) => (\n            <article\n              key={article.id}\n              className=\"relative grid-cols-5 mt-4 border border-gray-300 rounded-lg shadow-sm border-opacity-60 sm:grid p-7 lg:max-w-2xl sm:p-4 lg:col-span-2 lg:ml-20\"\n            >\n              <img\n                src={`http://localhost:5000/uploads/${article.imageUrn}`}\n                alt={article.imageUrn}\n                className=\"object-cover w-full rounded-lg max-h-32\"\n              />\n              <div className=\"self-center col-span-3 pt-5 sm:pt-0 sm:pl-10\">\n                <h2 className=\"mr-3 text-xl font-bold text-gray-800 capitalize\">\n                  <Link to={`/articles/${article.id}`}>{article.title}</Link>\n                </h2>\n                <Link to=\"/\" className=\"inline-block pt-2 mr-3 capitalize\">\n                  {article.username}\n                </Link>\n              </div>\n              <div className=\"justify-self-end\">\n                <img\n                  src=\"https://cdn4.iconfinder.com/data/icons/app-custom-ui-1/48/Bookmark-256.png\"\n                  alt=\"Bookmark\"\n                  className=\"absolute w-8 top-3 right-3 sm:relative sm:top-0 sm:right-0\"\n                />\n              </div>\n            </article>\n          ))}\n          <div className=\"flex justify-center mt-2\">\n            {!status && !error && (\n              <Pagination\